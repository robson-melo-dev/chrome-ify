{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "description": "Describe your chrome extension here",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_title": "My Chrome Extension",   
    "default_popup": "popup.html" 
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://www.google.com/",
    "https://*.google.com/"
  ]
}